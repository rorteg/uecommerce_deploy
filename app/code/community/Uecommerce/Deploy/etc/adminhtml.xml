    <acl>
        <resources>
            <all>
                <title>Allow Everything</title>
            </all>
            <admin>
                <children>
                    <system>
                        <children>
                            <config>
                                <children>
                                    <uecommerce_deploy module="Uecommerce_Deploy">
                                        <title>Uecommerce Deploy</title>
                                    </uecommerce_deploy>
                                </children>
                            </config>
                        </children>
                    </system>
                </children>
            </admin>
        </resources>
    </acl>
</config>
